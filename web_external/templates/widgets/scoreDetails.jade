table.table.table-striped.table-condensed.c-submission-score-table
  thead
    tr
      th
      each metric in metrics
        th
          a.c-metric-heading(metric="#{metric}", tabindex="0")= metric
  tbody
    - var i = 0
    each dataset in datasets
      tr
        th= dataset
        each metric in metrics
          td= getScoreForCell(dataset, metric)
      - i ++
